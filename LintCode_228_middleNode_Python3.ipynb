    "def middleNode(self, head):\n",
    "    # write your code here\n",
    "    if head is None:\n",
    "        return None\n",
    "    slow = head\n",
    "    fast = head.next\n",
    "    while fast is not None and fast.next is not None:\n",
    "        slow = slow.next\n",
    "        fast = fast.next.next\n",
    "    return slow"
   ]
  }
